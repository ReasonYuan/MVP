<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fq="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <com.fq.library.topbar.FQTopbar 
        android:id="@+id/fqt_base_bar"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        fq:background="@android:color/holo_blue_light"
        fq:titleTextColor="#FFFFFF"
        fq:leftText="返回"
        fq:leftTextColor="#FFFFFF"/>
    
    <FrameLayout
        android:id="@+id/fl_parent_container"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1" />

</LinearLayout>